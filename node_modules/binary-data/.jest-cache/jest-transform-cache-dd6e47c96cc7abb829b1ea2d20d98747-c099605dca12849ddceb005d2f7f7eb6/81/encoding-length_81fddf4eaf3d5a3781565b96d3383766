/**
 * Get the number of bytes to encode `obj` using `schema`.
 * @param {*} obj Any valid js object.
 * @param {Object} schema
 * @returns {number}
 */
function encodingLength(obj, schema) {
  /* istanbul ignore next */cov_bg5n9btlc.f[0]++;

  const context = /* istanbul ignore next */(cov_bg5n9btlc.s[4]++, new Metadata());

  /* istanbul ignore next */cov_bg5n9btlc.s[5]++;
